const int n=1e7;
int a[n+1]={0};
vector<int>  v;
void isprime()
{
        for(int i=2;i<=n;i++)
        {
            if(a[i]==0){
                for(int j=i*i;j<=n;j=j+i)
                    a[j]=1;
            }
        }
    for(int i=2;i<n;i++)
        if(a[i]==0)
            v.push_back(i);
}
               
